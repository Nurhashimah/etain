<h1>Listing schedules</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Scheduled on</th>
      <th>Location</th>
      <th>Lecturer by</th>
      <th>Max people</th>
      <th>Status</th>
      <th>User</th>
      <th>College</th>
      <th>Data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schedules.each do |schedule| %>
      <tr>
        <td><%= schedule.course_id %></td>
        <td><%= schedule.scheduled_on %></td>
        <td><%= schedule.location_id %></td>
        <td><%= schedule.lecturer_by %></td>
        <td><%= schedule.max_people %></td>
        <td><%= schedule.status %></td>
        <td><%= schedule.user_id %></td>
        <td><%= schedule.college_id %></td>
        <td><%= schedule.data %></td>
        <td><%= link_to 'Show', schedule %></td>
        <td><%= link_to 'Edit', edit_schedule_path(schedule) %></td>
        <td><%= link_to 'Destroy', schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Schedule', new_schedule_path %>
