<h1>Listing bookings</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Schedule</th>
      <th>Customer</th>
      <th>Payment</th>
      <th>Warranty</th>
      <th>Status</th>
      <th>User</th>
      <th>College</th>
      <th>Data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bookings.each do |booking| %>
      <tr>
        <td><%= booking.code %></td>
        <td><%= booking.schedule_id %></td>
        <td><%= booking.customer_id %></td>
        <td><%= booking.payment_id %></td>
        <td><%= booking.warranty_id %></td>
        <td><%= booking.status %></td>
        <td><%= booking.user_id %></td>
        <td><%= booking.college_id %></td>
        <td><%= booking.data %></td>
        <td><%= link_to 'Show', booking %></td>
        <td><%= link_to 'Edit', edit_booking_path(booking) %></td>
        <td><%= link_to 'Destroy', booking, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Booking', new_booking_path %>
