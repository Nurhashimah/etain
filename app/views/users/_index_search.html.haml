= search_form_for @search, url: users_path, class: "form-inline" do |f|
  %tr.search_row{style: "display: none;"}
    %td= f.text_field :name_cont, :class => "form-control"
    %td= f.text_field :email_cont, :class => "form-control"
    %td= f.text_field :employer_name_cont, :class => "form-control"
    %td= f.text_field :college_name_cont, :class => "form-control"
    %td= select_tag('q[roles_search]',options_for_select([[(t 'actions.all_records'),''],['Administration', 'administration'],['Trainer', 'trainer'],['Trainee', 'trainee']]), class: 'selectpicker',"data-width"=>'100px')
    %td
      %button.btn.btn-sm.btn-default.right{:type => "submit"}
        %i.fa.fa-search.fa-fw
